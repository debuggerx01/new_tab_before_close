{
  "name": "NewTabBeforeClose",
  "version": "1.1",
  "author": "DebuggerX",
  "description": "当关闭最后一个Tab时打开一个新标签页而不是关闭浏览器",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  }
}